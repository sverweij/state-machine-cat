<?xml version="1.0" encoding="UTF-8"?>
<scxml xmlns="http://www.w3.org/2005/07/scxml" version="1.0">
{{#each states}}
    <{{type}} id="{{id}}">
    {{#each ./onentries}}
        <onentry>{{.}}</onentry>
    {{/each}}
    {{#each ./onexits}}
        <onexit>{{.}}</onexit>
    {{/each}}
    {{#each ./transitions}}
        <transition {{#if event}}event="{{event}}" {{/if}}target="{{target}}"/>
    {{/each}}
    </{{type}}>
{{/each}}
</scxml>
