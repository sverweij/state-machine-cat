{
  "name": "state-machine-cat",
  "version": "2.2.0",
  "description": "write beautiful state charts",
  "main": "src/index.js",
  "scripts": {
    "depcruise": "depcruise --validate -- src test",
    "depcruise:graph": "depcruise --output-type dot --validate -- bin/smcat | dot -T svg > tmp_deps.svg && echo The dependency graph is in \\\"tmp_deps.svg\\\"",
    "lint": "eslint src test",
    "lint:fix": "eslint --fix src test",
    "npm-check-updates": "ncu --upgrade",
    "nsp": "nsp check",
    "postversion": "git push gitlab-mirror && git push --tags gitlab-mirror && git push && git push --tags",
    "preversion": "test `git branch | grep \"^* [a-zA-Z]\" | cut -c 3-` = 'master'",
    "test": "mocha --reporter spec --recursive test",
    "test:cover": "istanbul cover node_modules/mocha/bin/_mocha -- --timeout 4000 --recursive test"
  },
  "keywords": [
    "state",
    "state chart",
    "state diagram",
    "state machine",
    "finite state machine",
    "fsm"
  ],
  "author": "Sander Verweij",
  "license": "GPL-3.0",
  "devDependencies": {
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chai-json-schema": "1.5.0",
    "chai-xml": "0.3.1",
    "dependency-cruiser": "2.13.0",
    "eslint": "4.17.0",
    "eslint-plugin-compat": "2.2.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-mocha": "4.11.0",
    "eslint-plugin-security": "1.4.0",
    "istanbul": "0.4.5",
    "js-makedepend": "2.4.6",
    "mocha": "5.0.1",
    "npm-check-updates": "2.14.0",
    "nsp": "3.1.0",
    "pegjs": "0.10.0",
    "uglifyjs-webpack-plugin": "1.1.8",
    "webpack": "3.11.0",
    "webpack-monitor": "1.0.14"
  },
  "bin": {
    "smcat": "bin/smcat",
    "sm-cat": "bin/smcat",
    "sm_cat": "bin/smcat",
    "state-machine-cat": "bin/smcat"
  },
  "dependencies": {
    "ajv": "6.1.1",
    "commander": "2.14.1",
    "handlebars": "4.0.11",
    "semver": "5.5.0",
    "viz.js": "1.8.0"
  },
  "engines": {
    "node": ">=6"
  },
  "types": "types/state-machine-cat.d.ts",
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Firefox version",
    "last 1 Safari version"
  ],
  "homepage": "https://sverweij.gitlab.io/state-machine-cat/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sverweij/state-machine-cat"
  },
  "bugs": {
    "url": "https://github.com/sverweij/state-machine-cat/issues"
  }
}
