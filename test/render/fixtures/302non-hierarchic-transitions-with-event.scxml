<?xml version="1.0" encoding="UTF-8"?>
<scxml xmlns="http://www.w3.org/2005/07/scxml" version="1.0">
    <state id="a">
        <transition event="some_event" target="b"/>
        <transition event="some_other_event" target="b"/>
    </state>
    <state id="b">
        <transition event="and_back_again" target="a"/>
        <transition target="final"/>
    </state>
    <final id="final">
    </final>
</scxml>
